{
  "name": "bob-plug",
  "private": true,
  "homepage": "https://github.com/roojay520/bob-plug",
  "repository": "https://github.com/roojay520/bob-plug.git",
  "author": "roojay <roojay520@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "prettier:fix": "prettier --config .prettierrc.js --write .",
    "eslint": "eslint . -c .eslintrc.js --ext .ts,.tsx,.js",
    "eslint:fix": "eslint . -c .eslintrc.js --fix --ext .ts,.tsx,.js",
    "build": "esmo scripts/build.ts",
    "build:all": "yarn run build --all"
  },
  "keywords": [
    "bobplugin",
    "translate"
  ],
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
